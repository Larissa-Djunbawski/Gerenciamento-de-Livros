{
  "idLivro": 1,
  "idEstudante": 1,
  "dataAluguel": "2024-09-01",
  "dataDevolucao": "2024-09-10"
}

get http://localhost:5000/aluguel

put http://localhost:5000/aluguel/1
{
  "idLivro": 1,
  "idEstudante": 2,
  "dataAluguel": "2024-09-01",
  "dataDevolucao": "2024-09-10"
}

